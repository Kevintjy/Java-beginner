            <Button id="enter" mnemonicParsing="false" onAction="#enterButton" prefHeight="27.0" prefWidth="192.0" text="Enter" GridPane.columnIndex="1" GridPane.rowIndex="4" />
            <Button id="back" mnemonicParsing="false" onAction="#backButton" text="Back to Menu" GridPane.columnIndex="1" GridPane.rowIndex="4">
