          <RowConstraints maxHeight="153.0" minHeight="0.0" prefHeight="60.0" vgrow="SOMETIMES" />
          <RowConstraints maxHeight="323.0" minHeight="10.0" prefHeight="60.0" vgrow="SOMETIMES" />
            <RowConstraints maxHeight="323.0" minHeight="10.0" prefHeight="96.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label text="New user Name" GridPane.rowIndex="1" />
            <Label text="New user ID" GridPane.rowIndex="2" />
            <Label text="Authorization (Password)" GridPane.rowIndex="3" />
